{
  "compilerOptions": {
    "target": "ES2022",
    "module": "es2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "paths": {
      "@/*": ["www/*"]
    }
  }
}